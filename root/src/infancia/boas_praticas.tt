[% INCLUDE 'web/messages.tt' %]

<div>
<div class="row" id="boapratica">
    [%FOREACH bp IN best_pratices%]
    <div class="col-md-4">
        <div class="item">
            <a href="/[%bp.url%]">
                [%IF bp.image%]<img src="[% bp.image %]" />[%ELSE%]<img src="/static/infancia/boa-pratica-default.jpg" />[%END%]
                <h2>[% bp.name %]</h2>
                <p>[% bp.description %]...</p>
            </a>
        </div>
    </div>
    [%END%]
</div>
</div>