	<div id="breadcrumb">
    	<div class="contents">
            <ul class="xbreadcrumbs" id="breadcrumbs-top">
            </ul>
		    <div class="clear"></div>
        </div>
	</div>
	<div id="content">
		<div class="content">

        	<div id="indicador-dados">
                <div class="profile">
	            	<div class="content-fill">
                        <div class="title">[% indicator.name %]</div>
                        <div class="explanation"></div>
                        <div class="dados"><table class="tabela"></table></div>
					</div>
                </div>
            </div>
            <div class="clear"></div>
        	<div id="indicador-historico">
				<div class="title">Série Histórica de <span class="cidade"></span></div>
				<div class="content-fill">

					<div class="table">
						<div class="content-fill"></div>
					</div>

				</div>
            </div>
            <div class="clear"></div>
        	<div id="indicador-grafico">
				<div class="title">Gráfico de <span class="cidade"></span> &nbsp;<a href='/[% role %]/[% indicador.name_url %]/?view=graph&graphs=[% indicador.id %]' class='link'>comparar com outras cidades</a></div>
				<div class="content-fill">
					<div class="graph">
						<div class="content-fill">
                            <canvas id="main-graph" width='600' height='280'></canvas>
                            <div class="legend"></div>
                            <div class="clear"></div>
                        </div>
					</div>
				</div>
            </div>
            <div class="clear"></div>
        </div>    	
    </div>

	<script type="text/javascript">
			var userID = [% user.id %];
			var indicadorID = [% indicator.id %];
			var role = '[% find_role %]';
			var ref = 'indicador';
	</script>
